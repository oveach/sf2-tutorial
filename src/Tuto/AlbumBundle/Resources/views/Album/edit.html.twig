{% extends "TutoAlbumBundle::layout.html.twig" %}

{% block content %}

<h1>{{ title }}</h1>

<form action="{{ path('_edit', {'id': form.vars.value.id}) }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}

    <input type="submit" value="{{ submitValue }}"/>
</form>

{% endblock %}
